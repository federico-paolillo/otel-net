{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Tel": "Debug"
    }
  },
  "OpenTelemetry": {
    "Logging": {
      "Endpoint": "http://localhost:40318/v1/logs",
      "ExportTimeout": 1000
    },
    "Tracing": {
      "Endpoint": "http://localhost:40318/v1/traces",
      "ExportTimeout": 1000
    },
    "Metrics": {
      "Endpoint": "http://localhost:40318/v1/metrics",
      "ExportTimeout": 1000
    }
  }
}
